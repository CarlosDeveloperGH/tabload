<Window x:Class="Tabload.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tabload"
        mc:Ignorable="d"
        Title="Tabload" Height="350" Width="525">
    <Grid>
        <DockPanel>
            <ToolBarTray DockPanel.Dock="Top">
                <ToolBar IsOverflowOpen="False">
                    <Button ToolBar.OverflowMode="Never" Content="File" />
                    <Button ToolBar.OverflowMode="Never" Content="Settings" Click="OpenSettingsWindow"/>
                </ToolBar>
            </ToolBarTray>
            <TextBox AcceptsReturn="True" />
        </DockPanel>
        <Label x:Name="lblTitle" Content="Tabload" Margin="10,50,30,30"/>
    </Grid>
</Window>
