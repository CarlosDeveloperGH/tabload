<Window x:Class="Tabload.Views.Settings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tabload.Views"
        mc:Ignorable="d"
        Title="Settings"
        Height="450" Width="450"
        MaxHeight="450" MaxWidth="450"
        WindowStyle="None"
        Background="#282c34"
        ResizeMode="NoResize">
    <Grid>
        <Label x:Name="lblTitle" Content="Settings" Height="25" Margin="0,0,0,425" Foreground="{DynamicResource Static.Light.Primary}" Background="{DynamicResource Static.Dark.Secondary}"/>
        <Button x:Name="btnSaveChanges" Content="Save Changes" Margin="250,350,0,0" Width="125" Height="25" Click="SaveChanges" Style="{DynamicResource DarkButton}"/>
        <Label x:Name="lblInProgress" Content="This feature is coming soon" Margin="10,30,204,391" Foreground="{DynamicResource Static.Light.Primary}"/>
    </Grid>
</Window>
